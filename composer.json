        "intervention/image": "^2.3.14",
        "jdorn/sql-formatter": "^1.2",
        "jms/metadata": "^2.6.1",
        "jms/serializer-bundle": "^4.0.2",
        "jms/translation-bundle": "^1.6.2",
        "knplabs/knp-menu-bundle": "^3.2",
        "lcobucci/jwt": "^4.1.5",
        "league/flysystem": "^1.1.4",
        "litipk/php-bignumbers": "^0.8.6",
        "nette/utils": "^3.0",
        "nikic/php-parser": "^4.0",
        "nyholm/psr7": "^1.5",
        "object-calisthenics/phpcs-calisthenics-rules": "^3.1",
        "overblog/graphiql-bundle": "^0.2",
        "overblog/graphql-bundle": "^0.13",
        "phing/phing": "^2.17.3",
        "phpdocumentor/reflection-docblock": "^5.3.0",
        "presta/sitemap-bundle": "^3.3",
        "prezent/doctrine-translatable": "^3.0",
        "prezent/doctrine-translatable-bundle": "^1.4",
        "psr/log": "^1.0",
        "ramsey/uuid": "^4.3.1",
        "roave/better-reflection": "^5.4.0",
        "sensio/framework-extra-bundle": "^5.2",
        "shopsys/ordered-form": "^5.0",
        "shopsys/postgres-search-bundle": "^0.2",
        "sinergi/browser-detector": "^6.1",
        "snc/redis-bundle": "^3.2.2",
        "stof/doctrine-extensions-bundle": "^1.3.0",
        "superbalist/flysystem-google-storage": "^7.1",
        "symfony-cmf/routing": "^2.0.3",
        "symfony-cmf/routing-bundle": "^2.0.3",
        "symfony/browser-kit": "^4.4.0",
        "symfony/debug": "^4.4.0",
        "symfony/debug-bundle": "^4.4.0",
        "symfony/dotenv": "^4.4.0",
        "symfony/event-dispatcher": "^4.4.0",
        "symfony/flex": "^1.17",
        "symfony/form": "^4.4.0",
        "symfony/mailer": "^5.0",
        "symfony/monolog-bridge": "^4.4.0",
        "symfony/monolog-bundle": "^3.5.0",
        "symfony/property-info": "^4.4.0",
        "symfony/proxy-manager-bridge": "^4.4.25",
        "symfony/security-bundle": "^4.4.0",
        "symfony/translation": "^4.4.0",
        "symfony/twig-bundle": "^4.4.0",
        "symfony/validator": "^4.4.0",
        "symfony/web-link": "^4.4.0",
        "symfony/web-profiler-bundle": "^4.4.0",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/workflow": "^4.4.0",
        "tracy/tracy": "^2.4.13",
        "twig/extensions": "^1.5.1",
        "twig/twig": "^2.12",
        "webmozart/assert": "^1.4"
    },
    "require-dev": {
        "ext-pgsql": "*",
        "codeception/codeception": "^4.1.22",
        "codeception/module-db": "^1.0",
        "codeception/module-webdriver": "^1.0",
        "codeception/phpunit-wrapper": "^9.0.7",
        "friendsofphp/php-cs-fixer": "^3.3.1",
        "php-parallel-lint/php-parallel-lint": "^1.3.1",
        "phpstan/phpstan": "^1.6.8",
        "phpstan/phpstan-doctrine": "^1.3.4",
        "phpstan/phpstan-phpunit": "^1.1.1",
        "phpstan/phpstan-symfony": "^1.1.8",
        "phpunit/phpunit": "^9.5.20",
        "psr/event-dispatcher": "^1.0.0",
        "slevomat/coding-standard": "^6.3.5",
        "squizlabs/php_codesniffer": "^3.6.2",
        "sspooky13/yaml-standards": "^7.0.1",
        "symfony/var-dumper": "^4.4.0",
        "symplify/easy-coding-standard": "^10.2.2",
        "symplify/monorepo-builder": "^8.3.48",
        "zalas/phpunit-injector": "^2.0"
    },
    "conflict": {
        "symfony/symfony": "*",
        "friendsofphp/php-cs-fixer": ">=3.3.2",
        "doctrine/orm": "2.12.0"
    },
    "scripts": {
        "post-install-cmd": [
            "App\\Environment::checkEnvironment",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "App\\Environment::checkEnvironment",
            "@auto-scripts"
        ],
        "security-check": "security-checker security:check",
        "auto-scripts": {
            "php phing clean": "script",
            "shopsys:domains-urls:configure": "symfony-cmd",
            "ckeditor:install --clear=skip --release=full --tag=4.5.11": "symfony-cmd",
            "composer security-check": "script"
        }
    },
    "config": {
        "preferred-install": "dist",
        "component-dir": "project-base/web/components",
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ocramius/package-versions": true,
            "symfony/flex": true
        }
    },
    "extra": {
        "bin-dir": "project-base/bin",
        "config-dir": "project-base/config",
        "src-dir": "project-base/src",
        "var-dir": "project-base/var",
        "public-dir": "project-base/web",
        "symfony": {
            "allow-contrib": true,
            "require": "^4.4.0",
            "docker": false
        }
    },
    "replace": {
        "shopsys/form-types-bundle": "self.version",
        "shopsys/frontend-api": "self.version",
        "shopsys/google-cloud-bundle": "self.version",
        "shopsys/http-smoke-testing": "self.version",
        "shopsys/migrations": "self.version",
        "shopsys/plugin-interface": "self.version",
        "shopsys/product-feed-google": "self.version",
        "shopsys/product-feed-zbozi": "self.version",
        "shopsys/product-feed-heureka": "self.version",
        "shopsys/product-feed-heureka-delivery": "self.version",
        "shopsys/read-model": "self.version",
        "shopsys/framework": "self.version",
        "shopsys/coding-standards": "self.version"
    }
}
