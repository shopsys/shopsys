    },
    "conflict": {
        "symfony/symfony": "*",
        "guzzlehttp/psr7": "<=1.8.3 || >=2.0.0 <=2.1.0"
    },
    "scripts": {
        "post-install-cmd": [
            "App\\Environment::checkEnvironment",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "App\\Environment::checkEnvironment",
            "@auto-scripts"
        ],
        "security-check": "security-checker security:check",
        "auto-scripts": {
            "php phing clean": "script",
            "shopsys:domains-urls:configure": "symfony-cmd",
            "ckeditor:install --clear=skip --release=full --tag=4.5.11": "symfony-cmd",
            "composer security-check": "script"
        }
    },
    "config": {
        "preferred-install": "dist",
        "component-dir": "project-base/app/web/components",
        "sort-packages": true,
        "platform": {
            "php": "8.3.2"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ocramius/package-versions": true,
            "symfony/flex": true,
            "php-http/discovery": true,
            "symfony/runtime": true
        }
    },
    "extra": {
        "bin-dir": "project-base/app/bin",
        "config-dir": "project-base/app/config",
        "src-dir": "project-base/app/src",
        "var-dir": "project-base/app/var",
        "public-dir": "project-base/app/web",
        "symfony": {
            "allow-contrib": true,
            "require": "^5.4",
            "docker": false
        },
        "runtime": {
            "dotenv_path": "project-base/app/.env"
        }
    },
    "replace": {
        "shopsys/administration": "self.version",
        "shopsys/article-feed-luigis-box": "self.version",
        "shopsys/brand-feed-luigis-box": "self.version",
        "shopsys/category-feed-luigis-box": "self.version",
        "shopsys/coding-standards": "self.version",
        "shopsys/form-types-bundle": "self.version",
        "shopsys/framework": "self.version",
        "shopsys/frontend-api": "self.version",
        "shopsys/google-cloud-bundle": "self.version",
        "shopsys/http-smoke-testing": "self.version",
        "shopsys/luigis-box": "self.version",
        "shopsys/migrations": "self.version",
        "shopsys/plugin-interface": "self.version",
        "shopsys/product-feed-google": "self.version",
        "shopsys/product-feed-heureka": "self.version",
        "shopsys/product-feed-heureka-delivery": "self.version",
        "shopsys/product-feed-luigis-box": "self.version",
        "shopsys/product-feed-zbozi": "self.version",
        "shopsys/s3-bridge": "self.version"
