        "fp/jsformvalidator-bundle": "^1.6.1",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "gedmo/doctrine-extensions": "^3.5",
        "google/cloud-storage": "^1.10",
        "guzzlehttp/guzzle": "^6.3",
