    "require-dev": {
        "ext-pgsql": "*",
        "codeception/codeception": "^2.4.0",
        "codeception/phpunit-wrapper": "^7.0",
        "nette/utils": "^2.5",
        "phpstan/phpstan": "^0.11",
        "symfony/var-dumper": "^3.4",
        "symplify/easy-coding-standard-tester": "^5.2.17",
        "symplify/monorepo-builder": "^5.2.17",
        "shopsys/changelog-linker": "^5.5.0",
        "phpstan/phpstan-doctrine": "^0.11.2",
        "phpstan/phpstan-phpunit": "^0.11.2",
        "sspooky13/yaml-standards": "^4.2.5",
        "zalas/phpunit-injector": "^1.2"
    },
    "conflict": {
        "symfony/dependency-injection": "3.4.15|3.4.16",
        "symplify/better-phpdoc-parser": "5.4.14|5.4.15",
        "twig/twig": "2.6.1",
        "symfony/symfony": "*"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "App\\Environment::checkEnvironment",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "App\\Environment::checkEnvironment",
            "@auto-scripts"
        ],
        "auto-scripts": {
            "shopsys:domains-urls:configure": "symfony-cmd",
            "ckeditor:install --clear=skip --release=full --tag=4.5.11": "symfony-cmd",
            "cache:clear --no-warmup": "symfony-cmd",
            "security-checker security:check": "script"
        }
    },
    "config": {
        "preferred-install": "dist",
        "component-dir": "project-base/web/components",
        "sort-packages": true,
        "platform": {
            "php": "7.2"
        }
    },
    "extra": {
        "bin-dir": "project-base/bin",
        "config-dir": "project-base/config",
        "src-dir": "project-base/src",
        "var-dir": "project-base/var",
        "public-dir": "project-base/web",
        "symfony": {
            "allow-contrib": true,
            "require": "^3.4"
        },
        "incenteev-parameters": [
            {
                "file": "project-base/config/parameters.yml",
                "keep-outdated": true
            },
            {
                "file": "project-base/config/parameters_test.yml",
                "keep-outdated": true
            }
        ]
    },
    "replace": {
        "shopsys/backend-api": "self.version",
        "shopsys/form-types-bundle": "self.version",
        "shopsys/frontend-api": "self.version",
        "shopsys/google-cloud-bundle": "self.version",
        "shopsys/http-smoke-testing": "self.version",
        "shopsys/migrations": "self.version",
        "shopsys/plugin-interface": "self.version",
        "shopsys/product-feed-google": "self.version",
        "shopsys/product-feed-zbozi": "self.version",
        "shopsys/product-feed-heureka": "self.version",
        "shopsys/product-feed-heureka-delivery": "self.version",
        "shopsys/read-model": "self.version",
        "shopsys/framework": "self.version",
        "shopsys/coding-standards": "self.version"
    }
}
