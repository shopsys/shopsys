{
    "name": "shopsys/google-cloud-bundle",
    "type": "library",
    "description": "Shopsys Framework Google Cloud Bundle",
    "keywords": ["google cloud", "Shopsys Framework", "SSFW"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\GoogleCloudBundle\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "google/cloud-storage": "^1.10",
        "shopsys/form-types-bundle": "10.0.5",
        "shopsys/framework": "10.0.5",
        "shopsys/migrations": "10.0.5",
        "shopsys/plugin-interface": "10.0.5",
        "superbalist/flysystem-google-storage": "^7.1",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/http-kernel": "^4.4.13",
        "symfony/options-resolver": "^4.4"
    },
    "require-dev": {
        "shopsys/coding-standards": "10.0.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "11.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "ocramius/package-versions": true,
            "symfony/flex": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
