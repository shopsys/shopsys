{
    "name": "shopsys/product-feed-heureka",
    "type": "library",
    "description": "Shopsys Framework plugin interface for Heureka XML product feed generation",
    "keywords": ["Heureka", "product feed", "XML feed", "plugin", "Shopsys Framework", "SSFW"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\ProductFeed\\HeurekaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\ProductFeed\\HeurekaBundle\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "doctrine/collections": "^1.5",
        "doctrine/dbal": "^2.7",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/persistence": "^1.3.7",
        "shopsys/doctrine-orm": "^2.7.2",
        "shopsys/form-types-bundle": "9.0.x-dev",
        "shopsys/framework": "9.0.x-dev",
        "shopsys/migrations": "9.0.x-dev",
        "shopsys/plugin-interface": "9.0.x-dev",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/form": "^4.4",
        "symfony/http-kernel": "^4.4",
        "symfony/translation": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "shopsys/coding-standards": "9.0.x-dev",
        "twig/extensions": "^1.5.1",
        "twig/twig": "^2.12"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.0.x-dev"
        }
    }
}
