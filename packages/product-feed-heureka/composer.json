{
    "name": "shopsys/product-feed-heureka",
    "description": "Shopsys Framework plugin interface for Heureka XML product feed generation",
    "keywords": ["Heureka", "product feed", "XML feed", "plugin", "Shopsys Framework", "SSFW"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\ProductFeed\\HeurekaBundle\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.0",
        "ext-simplexml": "*",
        "shopsys/form-types-bundle": "~0.2.0",
        "shopsys/plugin-interface": "^0.3.0",
        "shopsys/product-feed-interface": "^0.5.0",
        "symfony/form": "^3.0",
        "symfony/framework-bundle": "^3.0",
        "symfony/translation": "^3.0",
        "symfony/validator": "^3.0"
    },
    "require-dev": {
        "shopsys/coding-standards": "3.0.2",
        "phpunit/phpunit": "^7.0",
        "twig/extensions": "1.3.0",
        "twig/twig": "1.34.0"
    }
}
