{
    "name": "shopsys/product-feed-zbozi",
    "type": "library",
    "description": "Shopsys Framework plugin interface for Zboží XML product feed generation",
    "keywords": ["Zboží", "product feed", "XML feed", "plugin", "Shopsys Framework", "SSFW"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\ProductFeed\\ZboziBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\ProductFeed\\ZboziBundle\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "doctrine/dbal": "^2.7",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/persistence": "~1.2.0",
        "shopsys/doctrine-orm": "^2.6.2",
        "shopsys/form-types-bundle": "9.0.x-dev",
        "shopsys/framework": "9.0.x-dev",
        "shopsys/migrations": "9.0.x-dev",
        "shopsys/plugin-interface": "9.0.x-dev",
        "symfony/config": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/form": "^3.4",
        "symfony/http-kernel": "^3.4",
        "symfony/translation": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "shopsys/coding-standards": "9.0.x-dev",
        "twig/extensions": "^1.5.1",
        "twig/twig": "^2.4.8"
    },
    "conflict": {
        "symfony/dependency-injection": "3.4.15|3.4.16",
        "twig/twig": "2.6.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.0.x-dev"
        }
    }
}
