{
    "name": "shopsys/read-model",
    "type": "library",
    "description": "[experimental] Read model layer for Shopsys Framework",
    "keywords": ["Read Model", "Shopsys Framework", "SSFW"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\ReadModelBundle\\": "src/"
        }
