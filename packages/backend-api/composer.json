{
    "name": "shopsys/backend-api",
    "type": "library",
    "description": "Shopsys Platform API support",
    "keywords": ["api", "Shopsys Platform", "SSFW"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\BackendApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\BackendApiBundle\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4.1",
        "ext-json": "*",
        "ext-pdo": "*",
        "doctrine/dbal": "^3.3.3",
        "friendsofsymfony/rest-bundle": "^2.5",
        "jms/serializer-bundle": "^2.4",
        "ramsey/uuid": "^4.3.1",
        "shopsys/form-types-bundle": "13.0.x-dev",
        "shopsys/framework": "13.0.x-dev",
        "shopsys/migrations": "13.0.x-dev",
        "shopsys/plugin-interface": "13.0.x-dev",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "shopsys/coding-standards": "13.0.x-dev"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
