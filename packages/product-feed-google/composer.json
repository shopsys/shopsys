{
    "name": "shopsys/product-feed-google",
    "type": "library",
    "description": "Shopsys Framework plugin interface for Google XML product feed generation",
    "keywords": ["Google", "product feed", "XML feed", "plugin", "Shopsys Framework", "SSFW"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\ProductFeed\\GoogleBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\ProductFeed\\GoogleBundle\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/molaux/PostgreSearchBundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/shopsys/doctrine2.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/shopsys/jparser.git"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-pdo": "*",
        "ext-json": "*",
        "doctrine/dbal": "^2.7",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/orm": "dev-doctrine-260-with-ddc1960-hotfix-and-ddc4005-hotfix as 2.6.0",
        "intaro/postgres-search-bundle": "@dev",
        "shopsys/form-types-bundle": "7.0.0-alpha2",
        "shopsys/framework": "7.0.0-alpha2",
        "shopsys/migrations": "7.0.0-alpha2",
        "shopsys/plugin-interface": "7.0.0-alpha2",
        "shopsys/product-feed-interface": "7.0.0-alpha2",
        "symfony/symfony": "^3.4.8",
        "timwhitlock/jparser": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0",
        "shopsys/coding-standards": "^3.0.2",
        "twig/extensions": "^1.3",
        "twig/twig": "^1.34"
    }
}
