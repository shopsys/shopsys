{
    "name": "shopsys/product-feed-heureka-delivery",
    "type": "library",
    "description": "Shopsys Platform plugin interface for Heureka delivery XML product feed generation",
    "keywords": ["Heureka", "product delivery feed", "XML feed", "plugin", "Shopsys Platform", "SSFW"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\ProductFeed\\HeurekaDeliveryBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\ProductFeed\\HeurekaDeliveryBundle\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "doctrine/orm": "^2.11.2",
        "shopsys/form-types-bundle": "15.0.x-dev",
        "shopsys/framework": "15.0.x-dev",
        "shopsys/migrations": "15.0.x-dev",
        "shopsys/plugin-interface": "15.0.x-dev",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-kernel": "^5.4",
        "twig/twig": "^3.14.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2.1"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
