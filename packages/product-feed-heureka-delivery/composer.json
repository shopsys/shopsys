{
    "name": "shopsys/product-feed-heureka-delivery",
    "type": "library",
    "description": "Shopsys Framework plugin interface for Heureka delivery XML product feed generation",
    "keywords": ["Heureka", "product delivery feed", "XML feed", "plugin", "Shopsys Framework", "SSFW"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\ProductFeed\\HeurekaDeliveryBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\ProductFeed\\HeurekaDeliveryBundle\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "doctrine/orm": "^2.11.2",
        "shopsys/form-types-bundle": "11.0.x-dev",
        "shopsys/framework": "11.0.x-dev",
        "shopsys/migrations": "11.0.x-dev",
        "shopsys/plugin-interface": "11.0.x-dev",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/http-kernel": "^4.4.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "shopsys/coding-standards": "11.0.x-dev",
        "twig/twig": "^2.12"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "11.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
