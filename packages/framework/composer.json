        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "ext-redis": "*",
        "ext-xml": "*",
        "arvenil/ninja-mutex": "^0.4.1",
        "barryvdh/elfinder-flysystem-driver": "^0.2",
        "commerceguys/intl": "^1.0.0",
        "composer/composer": "^1.6.0",
        "craue/formflow-bundle": "^3.0.3",
        "defuse/php-encryption": "^2.2.1",
        "doctrine/annotations": "^1.6",
