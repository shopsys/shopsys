{% block h1 %}{{ 'Parameter value of type color'|trans }}{% endblock %}


{% block main_content %}

    {{ form_start(form) }}
        {{ form_errors(form) }}

        {% embed '@ShopsysFramework/Admin/Inline/FixedBar/fixedBar.html.twig' %}
            {% block fixed_bar_content %}
