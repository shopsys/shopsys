{
    "name": "shopsys/frontend-api",
    "type": "library",
    "description": "Shopsys Framework Frontend API support",
    "keywords": ["api", "Shopsys Framework", "GraphQL", "SSFW"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\FrontendApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\FrontendApiBundle\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "lcobucci/jwt": "^3.3",
        "overblog/graphql-bundle": "^0.13",
        "overblog/graphiql-bundle": "^0.2",
        "shopsys/form-types-bundle": "9.0.4",
        "shopsys/framework": "9.0.4",
        "shopsys/migrations": "9.0.4",
        "shopsys/plugin-interface": "9.0.4",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/http-foundation": "^4.4",
        "symfony/http-kernel": "^4.4.13",
        "symfony/routing": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "shopsys/coding-standards": "9.0.4"
    }
}
