{
    "name": "shopsys/frontend-api",
    "type": "library",
    "description": "Shopsys Framework Frontend API support",
    "keywords": ["api", "Shopsys Framework", "GraphQL", "SSFW"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\FrontendApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\FrontendApiBundle\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "lcobucci/jwt": "^4.1.5",
        "overblog/graphql-bundle": "^0.13.3",
        "overblog/graphiql-bundle": "^0.2",
        "shopsys/form-types-bundle": "10.0.5",
        "shopsys/framework": "10.0.5",
        "shopsys/migrations": "10.0.5",
        "shopsys/plugin-interface": "10.0.5",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/http-foundation": "^4.4",
        "symfony/http-kernel": "^4.4.13",
        "symfony/routing": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "shopsys/coding-standards": "10.0.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "11.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
