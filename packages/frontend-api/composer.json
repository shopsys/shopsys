{
    "name": "shopsys/frontend-api",
    "type": "library",
    "description": "Shopsys Framework Frontend API support",
    "keywords": ["api", "Shopsys Framework", "GraphQL", "SSFW"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\FrontendApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\FrontendApiBundle\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "lcobucci/jwt": "^4.1.5",
        "overblog/graphql-bundle": "^1.0",
        "overblog/graphiql-bundle": "^0.3",
        "shopsys/form-types-bundle": "12.0.x-dev",
        "shopsys/framework": "12.0.x-dev",
        "shopsys/migrations": "12.0.x-dev",
        "shopsys/plugin-interface": "12.0.x-dev",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/routing": "^5.4",
        "webonyx/graphql-php": "^15.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "shopsys/coding-standards": "12.0.x-dev"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
