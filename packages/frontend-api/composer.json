{
    "name": "shopsys/frontend-api",
    "type": "library",
    "description": "Shopsys Framework Frontend API support",
    "keywords": [
        "api",
        "Shopsys Framework",
        "GraphQL",
        "SSFW"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\FrontendApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\FrontendApiBundle\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "overblog/graphql-bundle": "^0.12",
        "overblog/graphiql-bundle": "^0.1",
        "shopsys/form-types-bundle": "9.0.x-dev",
        "shopsys/framework": "9.0.x-dev",
        "shopsys/migrations": "9.0.x-dev",
        "shopsys/plugin-interface": "9.0.x-dev",
        "symfony/config": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/http-foundation": "^3.4",
        "symfony/http-kernel": "^3.4",
        "symfony/routing": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "shopsys/coding-standards": "9.0.x-dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.0.x-dev"
        }
    }
}
