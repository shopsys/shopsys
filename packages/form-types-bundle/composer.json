{
    "name": "shopsys/form-types-bundle",
    "type": "library",
    "description": "Shopsys Framework custom Symfony form types",
    "keywords": ["form", "form type", "Shopsys Framework", "SSFW"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\FormTypesBundle\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/options-resolver": "^5.4",
        "symfony/translation": "^5.4"
    },
    "require-dev": {
        "shopsys/coding-standards": "12.0.x-dev"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
