<createPacket>
    <apiPassword>{{ config.apiPassword }}</apiPassword>
    <packetAttributes>
        <number>{{ packet.number }}</number>
        <name>{{ packet.name }}</name>
        <surname>{{ packet.surname }}</surname>
        <email>{{ packet.email }}</email>
        <addressId>{{ packet.addressId }}</addressId>
        <value>{{ packet.value }}</value>
        <eshop>{{ config.sender }}</eshop>
    </packetAttributes>
</createPacket>
