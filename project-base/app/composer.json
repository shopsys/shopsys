{
    "name": "shopsys/project-base",
    "type": "project",
    "description": "Full-featured basis for your e-commerce project on Shopsys Platform",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "App\\": [
                "src/"
            ],
            "Overblog\\GraphQLBundle\\__DEFINITIONS__\\": "var/overblogCompiledClasses",
            "Sniffer\\": [
                "sniffer/"
            ]
        },
        "classmap": [
            "app/Environment.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "ext-redis": "*",
        "ext-xml": "*",
        "arvenil/ninja-mutex": "^0.6",
        "commerceguys/intl": "^2.0.0",
        "composer/composer": "^2.2.12",
        "doctrine/annotations": "^1.6.0",
        "doctrine/common": "^3.2.2",
        "doctrine/doctrine-bundle": "^2.5.7",
        "doctrine/doctrine-fixtures-bundle": "^3.0.2",
        "doctrine/doctrine-migrations-bundle": "^3.2.2",
        "doctrine/orm": "^2.11.2",
        "doctrine/persistence": "^2.4",
        "enlightn/security-checker": "^1.5",
        "fakerphp/faker": "^1.19.0",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "guzzlehttp/guzzle": "^7.5.0",
        "helios-ag/fm-elfinder-bundle": "^12.2",
        "heureka/overeno-zakazniky": "^4.0.1",
        "jms/serializer-bundle": "^4.0.2",
        "jms/translation-bundle": "^2.2.1",
        "league/flysystem": "^3.11.0",
        "league/flysystem-aws-s3-v3": "^3.12.2",
        "nette/utils": "^3.2",
        "overblog/dataloader-bundle": "^0.6.0",
        "presta/sitemap-bundle": "^3.3",
        "prezent/doctrine-translatable-bundle": "^1.4",
        "sentry/sentry-symfony": "^4.2.8",
        "shopsys/deployment": "^3.1.0",
        "shopsys/form-types-bundle": "16.0.x-dev",
        "shopsys/framework": "16.0.x-dev",
        "shopsys/frontend-api": "16.0.x-dev",
        "shopsys/jsformvalidator-bundle": "^1.7.0",
        "shopsys/administration": "16.0.x-dev",
        "shopsys/migrations": "16.0.x-dev",
        "shopsys/plugin-interface": "16.0.x-dev",
        "shopsys/brand-feed-luigis-box": "16.0.x-dev",
        "shopsys/category-feed-luigis-box": "16.0.x-dev",
        "shopsys/product-feed-google": "16.0.x-dev",
        "shopsys/product-feed-heureka": "16.0.x-dev",
        "shopsys/product-feed-heureka-delivery": "16.0.x-dev",
        "shopsys/product-feed-luigis-box": "16.0.x-dev",
        "shopsys/product-feed-zbozi": "16.0.x-dev",
        "shopsys/article-feed-luigis-box": "16.0.x-dev",
        "shopsys/luigis-box": "16.0.x-dev",
        "shopsys/s3-bridge": "16.0.x-dev",
        "snc/redis-bundle": "^4.4.1",
        "stof/doctrine-extensions-bundle": "^1.3.0",
        "symfony-cmf/routing": "^2.0.3",
        "symfony-cmf/routing-bundle": "^2.0.3",
        "symfony/debug-bundle": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/error-handler": "^5.4",
        "symfony/flex": "^1.17",
        "symfony/form": "^5.4",
        "symfony/http-client": "^5.4",
        "symfony/mailer": "^5.0",
        "symfony/messenger": "^5.4",
        "symfony/monolog-bridge": "^5.4",
        "symfony/monolog-bundle": "^3.5.0",
        "symfony/property-info": "^5.4",
        "symfony/proxy-manager-bridge": "^5.4",
        "symfony/runtime": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/service-contracts": "^2.5.2",
        "symfony/stopwatch": "^5.4",
        "symfony/translation": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/web-link": "^5.4",
        "symfony/web-profiler-bundle": "^5.4",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/workflow": "^5.4",
        "tracy/tracy": "^2.4.13",
        "tuscanicz/enum": "^2.1",
        "twig/twig": "^3.5.1 <3.10.0",
        "webmozart/assert": "^1.4"
    },
    "require-dev": {
        "ext-pgsql": "*",
        "ext-zip": "*",
        "codeception/codeception": "^5.0.10",
        "codeception/module-db": "^3.1.0",
        "codeception/module-webdriver": "^4.0",
        "codeception/phpunit-wrapper": "^9.0.7",
        "phpstan/phpstan": "^1.11.7",
        "phpstan/phpstan-doctrine": "^1.4.8.",
        "phpstan/phpstan-phpunit": "^1.4.0",
        "phpstan/phpstan-symfony": "^1.4.6",
        "phpunit/phpunit": "^11.2.1",
        "psr/event-dispatcher": "^1.0.0",
        "shopsys/coding-standards": "16.0.x-dev",
        "shopsys/http-smoke-testing": "16.0.x-dev",
        "sspooky13/yaml-standards": "^9.0",
        "symfony/browser-kit": "^5.4",
        "symfony/var-dumper": "^5.4",
        "zalas/phpunit-injector": "2.5.x-dev as 2.5.1"
    },
    "conflict": {
        "symfony/symfony": "*",
        "guzzlehttp/psr7": "<=1.8.3 || >=2.0.0 <=2.1.0"
    },
    "scripts": {
        "pre-install-cmd": [
            "./check-composer-json.sh"
        ],
        "pre-update-cmd": [
            "./check-composer-json.sh"
        ],
        "post-install-cmd": [
            "App\\Environment::checkEnvironment",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "App\\Environment::checkEnvironment",
            "@auto-scripts",
            "@security-check"
        ],
        "auto-scripts": {
            "php app/downloadPhing.php --phing-version=3.0.0": "script",
            "php phing clean": "script",
            "shopsys:domains-urls:configure": "symfony-cmd",
            "ckeditor:install --clear=skip --release=full --tag=4.5.11": "symfony-cmd"
        },
        "security-check": "security-checker security:check"
    },
    "config": {
        "preferred-install": "dist",
        "component-dir": "web/components",
        "sort-packages": true,
        "platform": {
            "php": "8.3.2"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ocramius/package-versions": true,
            "symfony/flex": true,
            "cweagans/composer-patches": true,
            "php-http/discovery": true,
            "symfony/runtime": true
        }
    },
    "extra": {
        "bin-dir": "bin",
        "config-dir": "config",
        "src-dir": "src",
        "var-dir": "var",
        "public-dir": "web",
        "symfony": {
            "allow-contrib": true,
            "require": "^5.4",
            "docker": false
        }
    }
}
