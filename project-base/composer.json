{
    "name": "shopsys/project-base",
    "type": "project",
    "description": "Full-featured basis for your e-commerce project on Shopsys Framework",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "App\\": [
                "src/"
            ]
        },
        "classmap": [
            "app/Environment.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "ext-redis": "*",
        "ext-xml": "*",
        "arvenil/ninja-mutex": "^0.4.1",
        "commerceguys/intl": "^1.0.0",
        "composer/composer": "^2.2.12",
        "craue/formflow-bundle": "^3.0.3",
        "doctrine/annotations": "^1.6.0",
        "doctrine/common": "^3.2.2",
        "doctrine/doctrine-bundle": "^2.5.7",
        "doctrine/doctrine-fixtures-bundle": "^3.0.2",
        "doctrine/doctrine-migrations-bundle": "^3.2.2",
        "doctrine/orm": "^2.11.2",
        "doctrine/persistence": "^2.4",
        "enlightn/security-checker": "^1.3",
        "fakerphp/faker": "^1.19.0",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "helios-ag/fm-elfinder-bundle": "^12.2",
        "heureka/overeno-zakazniky": "^4.0.1",
        "intervention/image": "^2.3.14",
        "jms/serializer-bundle": "^4.0.2",
        "jms/translation-bundle": "^1.6.2",
        "league/flysystem": "^3.11",
        "nyholm/psr7": "^1.5",
        "phing/phing": "^2.17.3",
        "presta/sitemap-bundle": "^3.3",
        "prezent/doctrine-translatable-bundle": "^1.4",
        "sensio/framework-extra-bundle": "^5.2",
        "shopsys/form-types-bundle": "12.0.x-dev",
        "shopsys/jsformvalidator-bundle": "^1.7.0",
        "shopsys/framework": "12.0.x-dev",
        "shopsys/frontend-api": "12.0.x-dev",
        "shopsys/migrations": "12.0.x-dev",
        "shopsys/plugin-interface": "12.0.x-dev",
        "shopsys/postgres-search-bundle": "^0.4",
        "shopsys/product-feed-google": "12.0.x-dev",
        "shopsys/product-feed-heureka": "12.0.x-dev",
        "shopsys/product-feed-heureka-delivery": "12.0.x-dev",
        "shopsys/product-feed-zbozi": "12.0.x-dev",
        "shopsys/read-model": "12.0.x-dev",
        "snc/redis-bundle": "^4.4.1",
        "stof/doctrine-extensions-bundle": "^1.3.0",
        "symfony-cmf/routing": "^2.0.3",
        "symfony-cmf/routing-bundle": "^2.0.3",
        "symfony/debug-bundle": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/flex": "^1.17",
        "symfony/form": "^5.4",
        "symfony/mailer": "^5.0",
        "symfony/monolog-bridge": "^5.4",
        "symfony/monolog-bundle": "^3.5.0",
        "symfony/property-info": "^5.4",
        "symfony/proxy-manager-bridge": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/translation": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/web-link": "^5.4",
        "symfony/web-profiler-bundle": "^5.4",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/workflow": "^5.4",
        "tracy/tracy": "^2.4.13",
        "twig/twig": "^3.5.0",
        "webmozart/assert": "^1.4"
    },
    "require-dev": {
        "ext-pgsql": "*",
        "ext-zip": "*",
        "codeception/codeception": "^4.1.22",
        "codeception/module-db": "^1.0",
        "codeception/module-webdriver": "^1.0",
        "codeception/phpunit-wrapper": "^9.0.7",
        "phpstan/phpstan": "^1.6.8",
        "phpstan/phpstan-doctrine": "^1.3.4",
        "phpstan/phpstan-phpunit": "^1.1.1",
        "phpstan/phpstan-symfony": "^1.1.8",
        "phpunit/phpunit": "^9.5.20",
        "psr/event-dispatcher": "^1.0.0",
        "shopsys/coding-standards": "12.0.x-dev",
        "shopsys/http-smoke-testing": "12.0.x-dev",
        "sspooky13/yaml-standards": "^7.0.1",
        "symfony/var-dumper": "^5.4",
        "symfony/browser-kit": "^5.4",
        "zalas/phpunit-injector": "^2.0"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "post-install-cmd": [
            "App\\Environment::checkEnvironment",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "App\\Environment::checkEnvironment",
            "@auto-scripts",
            "@security-check"
        ],
        "auto-scripts": {
            "php phing clean": "script",
            "shopsys:domains-urls:configure": "symfony-cmd",
            "ckeditor:install --clear=skip --release=full --tag=4.5.11": "symfony-cmd"
        },
        "security-check": "security-checker security:check"
    },
    "config": {
        "preferred-install": "dist",
        "component-dir": "web/components",
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "ocramius/package-versions": true,
            "symfony/flex": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "bin-dir": "bin",
        "config-dir": "config",
        "src-dir": "src",
        "var-dir": "var",
        "public-dir": "web",
        "symfony": {
            "allow-contrib": true,
            "require": "^5.4",
            "docker": false
        }
    }
}
