version: '2'
syncs:
    shopsys-framework-sync:
        unison_image: eugenmayer/unison:2.51.3-4.12.0-ARM64
        sync_userid: 501
        src: './'
        sync_excludes:
            - .git
            - .idea
            - .docker-sync
            - .DS_Store
            - node_modules
            - var/cache
            - var/elasticsearch-data
            - var/postgres-data
            - web
        host_disk_mount_mode: 'delegated'
